{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/js/index.ts",
            "cwd": "${workspaceFolder}/build_cmake/build_wasm",
            "preLaunchTask": "Build",
            "internalConsoleOptions": "openOnSessionStart",
            "outputCapture": "std",
            "outFiles": [
                "${workspaceFolder}/js/out/**/*.js"
            ]
        }
    ]
}